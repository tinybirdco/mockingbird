{
  "name": "@tinybirdco/mockingbird",
  "description": "Mockingbird",
  "version": "1.4.1",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./dist/Schema.json": "./dist/Schema.json"
  },
  "files": [
    "dist/**"
  ],
  "keywords": [
    "tinybird",
    "data",
    "generator",
    "kafka",
    "kinesis",
    "spanner"
  ],
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/tinybirdco/mockingbird#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/tinybirdco/mockingbird/packages/mockingbird"
  },
  "scripts": {
    "build": "tsup",
    "dev": "npm run build --watch",
    "clean": "rm -rf dist && rm -rf node_modules && rm -rf .turbo"
  },
  "dependencies": {
    "@aws-sdk/client-kinesis": "^3.699.0",
    "@aws-sdk/client-sns": "^3.699.0",
    "@faker-js/faker": "^8.4.1",
    "@google-cloud/spanner": "^7.16.0",
    "amqplib": "^0.10.5",
    "cross-fetch": "^4.0.0",
    "google-auth-library": "^9.15.0",
    "ts-json-schema-generator": "^2.3.0",
    "ts-morph": "^24.0.0",
    "zod": "^3.23.8",
    "lodash.get": "^4.4.2"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.6",
    "@types/lodash.get": "^4.4.7",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18"
  }
}
