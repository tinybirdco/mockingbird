{
  "name": "@tinybirdco/mockingbird",
  "description": "Mockingbird",
  "version": "2.0.0-rc.0",
  "type": "module",
  "main": "./dist/client/index.es.js",
  "module": "./dist/client/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/client/client.d.ts",
      "import": "./dist/client/index.es.js"
    },
    "./client": {
      "types": "./dist/client/client.d.ts",
      "import": "./dist/client/index.es.js"
    },
    "./server": {
      "types": "./dist/server/server.d.ts",
      "require": "./dist/server/index.es.js",
      "import": "./dist/server/index.es.js"
    }
  },
  "files": [
    "dist/**"
  ],
  "keywords": [
    "tinybird",
    "data",
    "generator",
    "kafka",
    "kinesis",
    "spanner"
  ],
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/tinybirdco/mockingbird#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/tinybirdco/mockingbird/packages/mockingbird"
  },
  "scripts": {
    "build:client": "tsc --project tsconfig.client.json && vite build --config vite.config.client.js",
    "build:server": "tsc --project tsconfig.server.json && vite build --config vite.config.server.js",
    "build": "npm run build:client && npm run build:server",
    "dev": "vite build --watch",
    "clean": "rm -rf dist && rm -rf node_modules && rm -rf .turbo"
  },
  "dependencies": {
    "@aws-sdk/client-kinesis": "^3.741.0",
    "@aws-sdk/client-sns": "^3.741.0",
    "@faker-js/faker": "^9.4.0",
    "@google-cloud/spanner": "7.17.1",
    "amqplib": "^0.10.5",
    "cross-fetch": "^4.1.0",
    "google-auth-library": "^9.15.1",
    "ts-json-schema-generator": "^2.3.0",
    "ts-morph": "^25.0.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.6",
    "rollup-plugin-visualizer": "^5.14.0",
    "typescript": "^5.7.3",
    "vite": "^6.0.11"
  },
  "engines": {
    "node": ">=22"
  },
  "packageManager": "pnpm@9.15.0"
}
